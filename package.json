{
	"name": "n8n-nodes-appwrite-io",
	"version": "0.1.0",
	"description": "n8n community node for Appwrite - interact with Appwrite databases directly from your n8n workflows with schema-based document creation",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"appwrite",
		"database",
		"backend",
		"baas",
		"workflow",
		"automation"
	],
	"license": "MIT",
	"homepage": "https://github.com/mkccl/n8n-nodes-appwrite-io#readme",
	"author": {
		"name": "Michael Krause",
		"email": "Michael.Krause@e-mail.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mkccl/n8n-nodes-appwrite-io.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"build:watch": "tsc --watch & gulp build:icons --watch",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/AppwriteApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Appwrite/Appwrite.node.js"
		]
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/request-promise-native": "~1.0.15",
		"@typescript-eslint/parser": "~5.45",
		"eslint-plugin-n8n-nodes-base": "^1.0.0",
		"gulp": "^4.0.2",
		"n8n-core": "^0.125.0",
		"n8n-workflow": "^0.107.0",
		"prettier": "^2.7.1",
		"typescript": "~4.8.4"
	},
	"dependencies": {
		"node-appwrite": "^20.2.1"
	}
}
